{
  "name": "@ministryofjustice/hmpps-electronic-monitoring-components",
  "version": "0.0.5",
  "description": "HMPPS Electronic Monitoring Frontend Components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/hmpps-electronic-monitoring-components.git"
  },
  "bugs": {
    "url": "https://github.com/ministryofjustice/hmpps-electronic-monitoring-components/issues"
  },
  "homepage": "https://github.com/ministryofjustice/hmpps-electronic-monitoring-components#readme",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "scripts": {
    "prepare": "husky",
    "lint": "eslint src --cache --max-warnings 0",
    "lint-fix": "eslint src --cache --max-warnings 0 --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "int-test": "cypress run --component --config-file integration_tests/cypress.config.ts",
    "int-test:ui": "cypress open --component --browser chrome --config-file integration_tests/cypress.config.ts",
    "vite": "vite",
    "build:css": "for file in src/components/**/styles/*.scss; do sass --no-source-map --quiet-deps --load-path=node_modules \"$file\" \"${file%.scss}.raw.css\"; done",
    "watch:css": "chokidar 'src/components/**/styles/**/*.scss' -c \"npm run build:css\"",
    "start-feature": "npm-run-all --parallel build:css watch:css vite",
    "build": "npm run build:css && vite build && tsc",
    "preview": "vite preview"
  },
  "keywords": [
    "web-component",
    "openlayers",
    "map",
    "moj",
    "govuk",
    "typescript",
    "frontend",
    "geodata",
    "gis",
    "component"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./map": {
      "types": "./dist/types/components/map/scripts/index.d.ts",
      "import": "./dist/components/map/index.es.js",
      "require": "./dist/components/map/index.cjs.js"
    },
    "./map/layers": {
      "types": "./dist/types/components/map/scripts/core/layers/index.d.ts",
      "import": "./dist/components/map/layers/index.es.js",
      "require": "./dist/components/map/layers/index.cjs.js"
    },
    "./map/ordnance-survey-auth": {
      "types": "./dist/types/components/map/server/ordnance-survey-auth/index.d.ts",
      "import": "./dist/components/map/ordnance-survey-auth/index.es.js",
      "require": "./dist/components/map/ordnance-survey-auth/index.cjs.js"
    }
  },
  "files": [
    "dist/",
    "dist/index.*",
    "dist/map/",
    "dist/types/",
    "dist/nunjucks/"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/qs": "^6.9.18",
    "axios": "^1.9.0",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "maplibre-gl": "^5.7.0",
    "ol-mapbox-style": "^13.0.1",
    "superagent": "^10.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@cypress/vite-dev-server": "^7.0.0",
    "@ministryofjustice/eslint-config-hmpps": "^1.0.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^30.1.2",
    "chokidar-cli": "^3.0.0",
    "cypress": "^15.4.0",
    "eslint": "^9.34.0",
    "fast-glob": "^3.3.3",
    "govuk-frontend": "^5.11.0",
    "husky": "^9.1.7",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "jest-html-reporter": "^4.3.0",
    "jest-junit": "^16.0.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^16.1.5",
    "npm-run-all": "^4.1.5",
    "ol": "^10.6.1",
    "prettier": "^3.6.2",
    "prettier-plugin-jinja-template": "^2.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "typescript": "^5.8.3",
    "vite": "^7.1.12",
    "vite-plugin-eslint2": "^5.0.4",
    "vite-plugin-static-copy": "^3.1.4",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "govuk-frontend": ">=5",
    "ol": "^10.6.1"
  },
  "peerDependenciesMeta": {
    "ol": {
      "optional": false
    },
    "govuk-frontend": {
      "optional": false
    }
  }
}
